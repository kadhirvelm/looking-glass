{
  "name": "@looking-glass/electron",
  "version": "1.0.0",
  "description": "The desktop application looking-glass is served in.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --project . -d && cp src/*.html dist/",
    "dev": "yarn build && yarn watch",
    "lint": "eslint . --ext .ts,.tsx",
    "start": "electron ./dist/index.js",
    "watch": "tsc --project . --watch & yarn start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kadhirvelm/looking-glass.git"
  },
  "author": "kadhirvelm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kadhirvelm/looking-glass/issues"
  },
  "homepage": "https://github.com/kadhirvelm/looking-glass#readme",
  "dependencies": {
    "@looking-glass/application-server": "^1.0.0",
    "electron": "^7.1.7"
  }
}
